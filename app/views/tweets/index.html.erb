<section id="pagebody" role="main">
    <section id="sidebar">
  		<div id="sponsors">
			<h3>Leaders</h3>

			<div class="roundbox">
				<% @users.each do |user| %>
				  <div class="logo">
                    <img src=<%= "#{@redis.hget(user[:member], "profile_image_url")}"%>  width="75"  height="75"/>
                    <h4><a href="http://twitter.com/<%= "#{@redis.hget(user[:member], "from_user")}"%>">
                      @<%= "#{@redis.hget(user[:member], "from_user")}"%> (<%= user[:score].to_i%>)
                    </a></h4>
				  </div>
				 <%end%>
			</div> <!-- .roundbox -->
		</div>
	</section>

      <h1 class="tk-adelle">Tweets</h1>

  <div id="sessions">
	  <h3 class="tk-adelle">People who are leading!!</h3>
	  	<%= render :partial => 'tweet', :locals => {:tweets => @tweets}%>
	  
	  <div>
  </div>

 </section>